{
  "name": "TradingPlatform",
  "version": "0.0.1",
  "description": "Trading platform",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "jest",
    "test-cov": "jest --coverage",
    "devserver": "webpack-dev-server --env.BROWSER=normal --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8999",
    "devserver-moz_ie": "webpack-dev-server --env.BROWSER=visitor_moz_ie --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base public --port 8999",
    "build": "webpack --env.BROWSER=normal --config webpack.config.prod.js -p",
    "build-moz_ie": "webpack --env.BROWSER=visitor_moz_ie --config webpack.config.prod.js -p",
    "start:starter": "cd starter && npm run devserver",
    "vdev": "concurrently \"npm run devserver\" \"npm run start:starter\"",
    "vdev-moz_ie": "concurrently \"npm run devserver-moz_ie\" \"npm run start:starter\"",
    "analyze-bundle": "webpack --env.BROWSER=normal --config webpack-analyze.config.prod.js -p",
    "analyze-bundle-moz_ie": "webpack --env.BROWSER=visitor_moz_ie --config webpack-analyze.config.prod.js -p"
  },
  "author": "Maxim Rozhaev",
  "license": "ISC",
  "engines": {
    "node": ">= 7.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.scss$|^.+\\.wav": "<rootDir>/tests/mocks/style-mock.js",
      "globals": {
        "window": true
      }
    }
  },
  "dependencies": {
    "node-static": "^0.7.10",
    "promise-polyfill": "6.0.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sass": "^1.0.0-beta.4",
    "eslint-plugin-react": "^7.8.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "autoprefixer": "^8.5.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.6",
    "jest": "^22.1.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.7.2",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "react-addons-test-utils": "15.5.1",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.5.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.11.0"
  }
}
